Plugin 'mxw/vim-jsx'
Plugin 'editorconfig/editorconfig-vim'
Plugin 'fleischie/vim-styled-components'
Plugin 'mitermayer/vim-prettier'

if count(g:sidds_bundle_groups, 'jssnip')
    " ES2015 code snippets
    Plugin 'epilande/vim-es2015-snippets'
